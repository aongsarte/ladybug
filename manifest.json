{
  "manifest_version": 2,

  "name": "GSE",
  "description": "This extension shows a Google image search result for the current page",
  "version": "1.0",

  "browser_action" : {
    "default_icon": "lady.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/"
  ]
}
